{
  "name": "puex",
  "version": "0.5.0",
  "description": "1kb Vuex alternative.",
  "repository": {
    "url": "egoist/puex",
    "type": "git"
  },
  "main": "dist/puex.cjs.js",
  "module": "dist/puex.m.js",
  "jsnext:main": "dist/puex.m.js",
  "cdn": "dist/puex.min.js",
  "unpkg": "dist/puex.min.js",
  "files": [
    "dist"
  ],
  "scripts": {
    "test": "jest && npm run lint",
    "lint": "xo",
    "prepublish": "npm run build",
    "build": "bili --format es,cjs,umd,umd-min --module-name puex --global.vue Vue",
    "example": "BABEL_ENV=example poi"
  },
  "author": "egoist <0x142857@gmail.com>",
  "license": "MIT",
  "jest": {
    "testEnvironment": "node"
  },
  "babel": {
    "env": {
      "example": {
        "presets": [
          [
            "vue-app",
            {
              "useBuiltIns": true
            }
          ]
        ]
      },
      "test": {
        "presets": [
          [
            "env",
            {
              "targets": {
                "node": "current"
              }
            }
          ]
        ]
      }
    }
  },
  "poi": {
    "entry": "example/index.js",
    "dist": "example/dist"
  },
  "devDependencies": {
    "babel-preset-env": "^1.4.0",
    "bili": "^1.6.3",
    "eslint-config-rem": "^3.0.0",
    "jest-cli": "^19.0.0",
    "poi": "^9.1.3",
    "xo": "^0.18.0"
  },
  "xo": {
    "extends": "rem/prettier",
    "envs": [
      "jest"
    ],
    "ignores": [
      "example/**"
    ],
    "rules": {
      "import/prefer-default-export": 0
    }
  },
  "peerDependencies": {
    "vue": "^2.4.2"
  }
}
